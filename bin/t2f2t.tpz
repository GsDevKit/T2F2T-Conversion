#
  set solologin on
  set u SystemUser p swordfish
  login

	iferr 1 stk
	iferr 2 stack
	iferr 3 exit

	display oops

	run
	"install the script code into the topaz solo image"
	[
		(Rowan
			projectFromUrl: 'file:$binDir/..', specFile
			gitUrl: 'file:$binDir/../rowan/specs/tonel.ston') load ]
		on: Warning
		do: [:ex | 
			"ignore warnings during load"
			ex resume ]
%

run
	T2F2TConversion executeCommandLine
%
