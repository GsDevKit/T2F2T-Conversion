#
  set solologin on
  set u SystemUser p swordfish
  login

run
	| rootDir specFile |
	"install the script code into the topaz solo image"
	rootDir := '$binDir/..' asFileReference pathString.
	specFile := rootDir, '/rowan/specs/T2F2T-Conversion.ston' asFileReference pathString.
	(Rowan
		projectFromUrl: 'file://', specFile
		gitUrl: 'file://', rootDir) load
%

run
	T2F2TConversion executeCommandLine
%
